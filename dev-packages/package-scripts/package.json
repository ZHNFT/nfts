    "@types/node": "^16.4.0",
    "eslint": "^7.31.0",
    "ts-node": "^10.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@raydium/eslint-config/profiles/node.js"
    ],
    "parserOptions": {
      "tsconfigRootDir": ".",
      "project": "./tsconfig.json"
    }
  }
}
