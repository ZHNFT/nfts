{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://json-schema.org/draft/2020-12/output/schema",
  "description": "Schema for gmf project",
  "type": "object",
  "properties": {
    "buildPath": {
      "type": "string"
    },
    "buildTarget": {
      "type": "string",
      "oneOf": []
    },
    "plugins": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "pluginName": {
            "type": "string"
          }
        }
      }
    }
  }
}
