{
  "name": "@nfts/mono-package",
  "version": "0.0.0",
  "bin": {
    "mono-package": "/bin/mono-package"
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch --declaration --declarationDir dist",
    "dts": "npx tsc --declaration --emitDeclarationOnly --declarationDir dist"
  },
  "$devDependencies": {
    "@nfts/command-line": "workspace:*",
    "@nfts/eslint-config": "workspace:*",
    "@nfts/node-arg-parser": "workspace:*",
    "@nfts/node-utils-library": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "~17.0.5",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
